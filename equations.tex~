\documentclass{article}
\begin{document}
\title{Model equations}
\author{Himanshu}
\maketitle
\begin{equation}
    \label{Voltage equation}
    \frac{dV}{dt} = (-(V-Vr)+x)/taum
\end{equation}
\begin{equation}
    \label{Conductance equation}
    \frac{dx}{dt} = (-x+y)/taupsp
\end{equation}
\begin{equation}
    \label{Synapse equation}
    \frac{dy}{dt} = -y/taupsp + 25.27 + \sqrt{39.24}*nrand
\end{equation}
\end{document}
